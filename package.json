{
  "name": "my-monorepo",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "npm run application",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "clean": "rimraf dist",
    "serve:app": "ionic serve",
    "watch-lib:apis": "ng build lib-apis --watch",
    "watch-lib:components": "wait-on dist/lib-apis/package.json && ng build lib-components --watch",
    "watch-all": "npm run clean && run-p watch-lib:*",
    "serve:application": "wait-on dist/lib-components/package.json && rsync -a ./libraries/lib-components/src/assets/ ./dist/lib-components/assets/ && rsync -a ./libraries/lib-components/src/assets/ ./projects/application/src/assets/ && ionic serve --project application",
    "application": "npm run clean && run-p watch-lib:* serve:application"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^11.1.2",
    "@angular/common": "^11.1.2",
    "@angular/compiler": "^11.1.2",
    "@angular/core": "^11.1.2",
    "@angular/forms": "^11.1.2",
    "@angular/platform-browser": "^11.1.2",
    "@angular/platform-browser-dynamic": "^11.1.2",
    "@angular/router": "^11.1.2",
    "@ionic-native/core": "^5.31.1",
    "@ionic-native/diagnostic": "^5.31.1",
    "@ionic-native/geolocation": "^5.31.1",
    "@ionic-native/google-plus": "^5.31.1",
    "@ionic-native/splash-screen": "^5.31.1",
    "@ionic-native/status-bar": "^5.31.1",
    "@ionic/angular": "^5.5.4",
    "@ionic/storage": "^2.3.1",
    "async-wait-until": "^1.2.4",
    "core-js": "^2.5.4",
    "rxjs": "~6.6.3",
    "tslib": "^1.9.0",
    "uuid": "^7.0.1",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/architect": "^0.1100.2",
    "@angular-devkit/build-angular": "^0.1100.2",
    "@angular-devkit/core": "^11.1.2",
    "@angular-devkit/schematics": "^11.1.2",
    "@angular-eslint/builder": "1.0.0",
    "@angular-eslint/eslint-plugin": "1.0.0",
    "@angular-eslint/eslint-plugin-template": "1.0.0",
    "@angular-eslint/schematics": "1.0.0",
    "@angular-eslint/template-parser": "1.0.0",
    "@angular/cli": "^11.1.2",
    "@angular/compiler": "^11.1.2",
    "@angular/compiler-cli": "11.0.2",
    "@angular/language-service": "11.0.2",
    "@ionic/angular-toolkit": "^3.0.0",
    "@types/core-js": "^2.5.3",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^10.14.19",
    "@typescript-eslint/eslint-plugin": "4.3.0",
    "@typescript-eslint/eslint-plugin-tslint": "^4.9.1",
    "@typescript-eslint/parser": "4.3.0",
    "babel-eslint": "^10.1.0",
    "codelyzer": "^6.0.1",
    "eslint": "^7.18.0",
    "eslint-plugin-cypress": "^2.11.2",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsdoc": "30.7.6",
    "eslint-plugin-prefer-arrow": "1.2.2",
    "eslint-plugin-promise": "^4.2.1",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.1.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "minimist": "^1.2.0",
    "ng-packagr": "^11.1.4",
    "npm-run-all": "^4.1.5",
    "protractor": "~5.4.0",
    "rimraf": "^3.0.2",
    "tsickle": "^0.37.0",
    "tslint": "^6.1.3",
    "typescript": "~4.0.5",
    "wait-on": "^4.0.1"
  }
}
